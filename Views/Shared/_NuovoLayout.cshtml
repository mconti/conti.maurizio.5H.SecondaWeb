<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Fablab Romagna A.P.S.</title>
    <link rel="stylesheet" href="~/html5up-verti/assets/css/main.css" />

    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
</head>
	<body class="is-preload homepage">
        <div id="page-wrapper">

            <!-- Header -->
            <div id="header-wrapper">
                <header id="header" class="container">

                    <!-- Logo -->
                    <div id="logo">
                        <img src="~/images/logosito.png" />
                    </div>

                    <!-- Nav -->
                    <nav id="nav">
                        <ul>
                            <li class="current"><a asp-controller="Home" asp-action="Index">Benvenuti</a></li>
                            @*<li>
                                <a href="#">Progetti</a>
                                <ul>
                                    <li><a href="#">Making</a></li>
                                    <li><a href="#">Disegno e stampa 3D</a></li>
                                    <li>
                                        <a href="#">Scuola</a>
                                        <ul>
                                            <li><a href="#">PCTO</a></li>
                                            <li><a href="#">Didattica laboratoriale a distanza</a></li>
                                            <li><a href="#">Sviluppo web</a></li>
                                        </ul>
                                    </li>
                                    <li><a href="#">Sviluppo hardware</a></li>
                                </ul>
                            </li>*@

                            <li><a asp-controller="Home" asp-action="Storia">Storia</a></li>
                            <li><a asp-controller="Home" asp-action="Blog">Blog</a></li>
                            <li><a asp-controller="Home" asp-action="Eventi">Eventi</a></li>
                            <li><a asp-controller="Account" asp-action="Registra">Registrati</a></li>
                            <li><a asp-controller="Account" asp-action="Login">Login</a></li>
                            <li><a asp-controller="Account" asp-action="Logout">Logout</a></li>

                        </ul>
                    </nav>

                </header>
            </div>

            <!-- Main -->
            <div id="content">
                <section class="last">
                    @RenderBody()
                </section>
            </div>

            <!-- Footer -->
            <div id="footer-wrapper">
                <footer id="footer" class="container">
                    <div class="row">
                        <div class="col-12">
                            <div id="copyright">
                                <ul class="menu">
                                    <li>&copy; Fablab Romagna Associazione di promozione sociale - 2014-2021</li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </footer>
            </div>

        </div>

		<!-- Scripts -->

			<script src="~/html5up-verti/assets/js/jquery.min.js"></script>
			<script src="~/html5up-verti/assets/js/jquery.dropotron.min.js"></script>
			<script src="~/html5up-verti/assets/js/browser.min.js"></script>
			<script src="~/html5up-verti/assets/js/breakpoints.min.js"></script>
			<script src="~/html5up-verti/assets/js/util.js"></script>
			<script src="~/html5up-verti/assets/js/main.js"></script>

	</body>
</html>